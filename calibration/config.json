{
    "simulator": "workflow-simulator-for-calibration",
    "simulator_docker_image": "wrenchproject/workflow-calibration",
    "config": "data/sample_input.json",
    "sampling": "uniform",
    "seed": 0,
    "calibration_ranges": {
        "platform": {
            "num_cores": {
                "min": 16,
                "max": 16,
                "scale": "linear"
            },
            "scheduling_overhead": {
                "min": 0,
                "max": 60000,
                "scale": "linear",
                "unit" : "ms"
            },
            "reference_flops": {
                "min": 3,
                "max": 3,
                "scale": "log2",
                "unit" : "Gf"
            },
            "speed": {
                "min": 3,
                "max": 10,
                "scale": "log2",
                "unit" : "Gf"
            },
            "bandwidth_submit_disk_read": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "bandwidth_submit_disk_write": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "bandwidth_compute_host_disk_read": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "bandwidth_compute_host_write": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "bandwidth_out_of_submit": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "latency_out_of_submit": {
                "min": 0,
                "max": 100,
                "scale": "linear",
                "unit": "us"
            },
            "bandwidth_to_compute_hosts": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "latency_to_compute_hosts": {
                "min": 0,
                "max": 100,
                "scale": "linear",
                "unit": "us"
            },
            "bandwidth_submit_to_compute_host": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "latency_submit_to_compute_host": {
                "min": 0,
                "max": 100,
                "scale": "linear",
                "unit": "us"
            },
            "bandwidth": {
                "min": 3,
                "max": 20,
                "scale": "log2",
                "unit" : "MBps"
            },
            "latency": {
                "min": 0,
                "max": 100,
                "scale": "linear",
                "unit": "us"
            }
        },
        "payloads": {
            "min": 0,
            "max": 20,
            "scale": "log2",
            "unit": "B"
        },
        "properties": {
            "submit_properties": {
                "SimpleStorageServiceProperty::MAX_NUM_CONCURRENT_DATA_CONNECTIONS": {
                    "min": 1,
                    "max": 64,
                    "scale": "linear"
                },
                "StorageServiceProperty::BUFFER_SIZE": {
                    "min": 100,
                    "max": 100,
                    "scale": "log2",
                    "unit": "MB"
                }
            },
            "bare_metal_properties": {
                "BareMetalComputeServiceProperty::THREAD_STARTUP_OVERHEAD": {
                    "min": 1,
                    "max": 10000,
                    "scale": "linear",
                    "unit" : "ms"
                }
            },
            "htcondor_properties": {
                "HTCondorComputeServiceProperty::NEGOTIATOR_OVERHEAD": {
                    "min": 1,
                    "max": 60000,
                    "scale": "linear",
                    "unit" : "ms"
                },
                "HTCondorComputeServiceProperty::GRID_PRE_EXECUTION_DELAY": {
                    "min": 1,
                    "max": 60000,
                    "scale": "linear",
                    "unit" : "ms"
                },
                "HTCondorComputeServiceProperty::GRID_POST_EXECUTION_DELAY": {
                    "min": 1,
                    "max": 60000,
                    "scale": "linear",
                    "unit" : "ms"
                }
            }
        }
    }
}
